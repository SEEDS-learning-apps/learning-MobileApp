<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/dashboardFragment">

    <fragment
        android:id="@+id/chatFragment"
        android:name="com.example.chat_bot.Activities.HomePage.ChatFragment"
        android:label="Chat" >
        <action
            android:id="@+id/action_chatFragment_to_exerciseFragment"
            app:destination="@id/exerciseFragment" />
    </fragment>

    <fragment
        android:id="@+id/exerciseFragment"
        android:name="com.example.chat_bot.Activities.HomePage.ExerciseFragment"
        android:label="Exercise" >

        <action
            android:id="@+id/action_exerciseFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
    </fragment>

    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.example.chat_bot.Ac.DashboardFragment"
        android:label="Dashboard" >
        <action
            android:id="@+id/action_dashboardFragment_to_downloadQuizActivity"
            app:destination="@id/downloadQuizActivity" />
    </fragment>

    <fragment
        android:id="@+id/chatzFragment"
        android:name="com.example.chat_bot.Activities.HomePage.Chatzfrag"
        android:label="Dashboard" />
    <activity
        android:id="@+id/downloadQuizActivity"
        android:name="com.example.chat_bot.Activities.acivity.downloadQuizActivity"
        android:label="activity_download_quiz"
        tools:layout="@layout/activity_download_quiz" />

</navigation>